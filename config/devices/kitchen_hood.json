{
  "device_name": "Kitchen Hood",
  "device_type": "broadlink_kitchen_hood",
  "device_id": "kitchen_hood",
  "alias": "kitchen_hood",
  "mqtt_progress_topic": "/devices/kitchen_hood/controls/progress",
  "broadlink": {
    "host": "192.168.110.131",
    "mac": "EC:0B:AE:EE:21:66",
    "device_class": "0x520b"
  },
  "commands": {
    "light_control": {
      "topic": "/devices/kitchen_hood/controls/light",
      "actions": [
        {
          "name": "light_on",
          "condition": "payload == '1'",
          "rf_code": "",
          "description": "Turn light on"
        },
        {
          "name": "light_off",
          "condition": "payload == '0'",
          "rf_code": "",
          "description": "Turn light off"
        }
      ]
    },
    "speed_control": {
      "topic": "/devices/kitchen_hood/controls/speed",
      "actions": [
        {
          "name": "speed_change",
          "condition": "payload == '1'",
          "speed": 1,
          "rf_code": "",
          "description": "Set hood speed to 1"
        },
        {
          "name": "speed_change",
          "condition": "payload == '2'",
          "speed": 2,
          "rf_code": "",
          "description": "Set hood speed to 2"
        },
        {
          "name": "speed_change",
          "condition": "payload == '3'",
          "speed": 3,
          "rf_code": "",
          "description": "Set hood speed to 3"
        },
        {
          "name": "speed_change",
          "condition": "payload == '4'",
          "speed": 4,
          "rf_code": "",
          "description": "Set hood speed to 4"
        },
        {
          "name": "hood_off",
          "condition": "payload == '0'",
          "rf_code": "",
          "description": "Turn hood off"
        }
      ]
    }
  }
} 