{
  "device_name": "Kitchen Hood",
  "device_id": "kitchen_hood",
  "alias": "kitchen_hood",

  "device_class": "BroadlinkKitchenHood",
  "config_class": "BroadlinkKitchenHoodConfig",
  "broadlink": {
    "host": "192.168.110.131",
    "mac": "EC:0B:AE:EE:21:66",
    "device_code": "0x520b"
  },
  "rf_codes": {
    "light": {
      "on": "",
      "off": ""
    },
    "speed": {
      "0": "",
      "1": "",
      "2": "",
      "3": "",
      "4": ""
    }
  },
  "commands": {
    "set_light": {
      "action": "set_light",
      "description": "Control kitchen hood light",
      "params": [
        {
          "name": "state",
          "type": "string",
          "required": true,
          "description": "Light state - 'on' or 'off'",
          "default": "off"
        }
      ]
    },
    "set_speed": {
      "action": "set_speed",
      "description": "Control kitchen hood speed",
      "params": [
        {
          "name": "level",
          "type": "range",
          "min": 0,
          "max": 4,
          "required": true,
          "description": "Speed level (0-4, where 0 is off)"
        }
      ]
    }
  },
  "wb_controls": {
    "set_light": {
      "type": "switch",
      "title": {"en": "Kitchen Light"},
      "order": 1
    },
    "set_speed": {
      "type": "range",
      "min": 0,
      "max": 4,
      "title": {"en": "Fan Speed"},
      "units": "",
      "order": 2
    }
  }
}